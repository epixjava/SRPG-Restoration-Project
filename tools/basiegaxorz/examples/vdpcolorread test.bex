	pallettes colors, 0, 0, 16
	
	ink 1
	for a=0 to 15
		locate a,0
		print hex$(vdpcolorread(a))
	next

	for a=0 to 15
		locate a,10
		print hex$(vdpcolorread(a+16))
	next


	disable screen
	cls
	pallettes pallette,1,0,16
	locate 0,0
	print "APA Drawing Plot Test"
	i=256
	for y=2 to 26
		for x=0 to 39
			drawtile i+8192,x,y
			i++
		next
	next
	p=0
	enable screen
	while 1
		p=p+1 [and] 15
		a=0
		b=1
		c=0
		for d=0 to 10
			for i=0 to 1102 step 2
				reload sin_table,i
				readint y
				y=y/b+c
				reload cos_table,i
				readint x
				x=x/b+c
				gosub draw_dot
				a=a+1
				if a=552 then a=0:c=184/b>>1:b=b+1
			next
		next
	wend
	end

draw_dot:
	dy=y/8*1280
	ddy=y%8<<2
	dx=x/8<<5
	ddx=x%8>>2<<1
	dddx=x%4
	if dddx=0 then dddx=4096:ddddx=&h0FFF else
		if dddx=1 then dddx=256:ddddx=&hF0FF else
	 		if dddx=2 then dddx=16:ddddx=&hFF0F else dddx=1:ddddx=&hFFF0
	 	endif
	endif
	old=vdpramread(8192+dy+ddy+dx+ddx) [and] ddddx
	vdpramwrite 8192+dy+ddy+dx+ddx,p*dddx+old
	return

